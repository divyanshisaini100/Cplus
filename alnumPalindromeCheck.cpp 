#include<bits/stdc++.h>
#include<string>


using namespace std;



bool alnumPalindromeCheck(string s){

    int start= 0 , end = s.length()-1;
    while(start<end){

    
     if(s[start] != s[end])
     {return false;}

    }
    return true;

    }
    
int main(){
    
    string name;
    cin>>name;
    cout<<alnumPalindromeCheck(name);
    
}    